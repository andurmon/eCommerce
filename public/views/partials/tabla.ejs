<table class="table table-hover">
    <tr><th>Titulo:</th> <th>Precio:</th><th>Imagen</th></tr>
    
    <% products.forEach((product)=>{ %>
        <tr>
            <td> <%= product.title %> </td>
            <td> <%= product.price %> </td>
            <td>
                <img width="100" src=<%= product.thumbnail %>> </img>
            </td>
        </tr>
    <% }) %>
</table>